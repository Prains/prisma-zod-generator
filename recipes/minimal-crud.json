{
  "mode": "minimal",
  "output": "./prisma/generated",
  "pureModels": true,
  "variants": {
    "pure": { "enabled": true, "suffix": ".model" },
    "input": { "enabled": true, "suffix": ".input", "excludeFields": ["id", "createdAt", "updatedAt"] },
    "result": { "enabled": false, "suffix": ".result" }
  }
}
